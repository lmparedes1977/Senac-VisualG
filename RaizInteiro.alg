algoritmo "Raiz Inteiro"
// Função :
// Autor :
// Data : 05/10/2022
// Seção de Declarações 
var

   numero : inteiro
   raiz, raizReal: real

inicio

   numero <- 111
   raiz <- RaizQ(numero)
   raizReal <- Int(raiz)
   
   Escreval (raiz)
   Escreval (raizReal)


fimalgoritmo
