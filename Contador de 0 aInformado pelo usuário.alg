algoritmo "Contar0a10"
// Função :
// Autor :
// Data : 13/10/2022
// Seção de Declarações 
var

   i, contador: inteiro


inicio

   Escreva ("Digite um número e aperta enter pra tu ver: ")
   Leia (contador)

   i := 0
   
   
   Enquanto (i <= contador) faca
      Escreval (i)

      i := i +  1
      
   FimEnquanto


fimalgoritmo
