algoritmo "Contar0a10"
// Função :
// Autor :
// Data : 13/10/2022
// Seção de Declarações 
var

   contador: inteiro


inicio

   contador := 10
   
   
   Enquanto (contador >= 0) faca

      Se (contador % 2 = 0) entao
         Escreval (contador)
      FimSe
      
      contador := contador - 1
      
   FimEnquanto


fimalgoritmo
