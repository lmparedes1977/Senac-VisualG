Algoritmo "Enquanto"
Var
   Nomes : vetor [0..4] de caractere
   I : inteiro
Inicio
   Nomes[0] <- "Pai"
   Nomes[1] <- "Mãe"
   Nomes[2] <- "Ju"
   Nomes[3] <- "Jé"
   Nomes[4] <- "Cãs"
   
   I <- 0
   Enquanto I < 5 Faca
      Escreval (Nomes[I])
      I <- I + 1
   
   FimEnquanto
   
   
Fimalgoritmo