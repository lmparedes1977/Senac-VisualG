Algoritmo "Vetor"
Var
   Nomes : vetor [0..30] de caractere
   
Inicio
   Nomes[0] <- "Pai"
   Nomes[1] <- "Mãe"
   Nomes[2] <- "Ju"
   Nomes[3] <- "Jé"

   Escreval (Nomes[0],", ",Nomes[1],", ",Nomes[2],", ",Nomes[3])
   Escreval (Nomes[1])
   Escreval (Nomes[2])
   Escreval (Nomes[3])
   
Fimalgoritmo